#!/bin/sh

export PACKAGE=ffmpeg
export PORTS=" ffmpeg-free ffmpegthumbnailer ffmpegthumbnailer-libs libavcodec-free libavdevice-free libavfilter-free libavformat-free libavutil-free libpostproc-free libswresample-free libswscale-free freeglut libavc1394 libcaca libdc1394 openal-soft"
repackage -e

returnval=$?

exit $returnval
