. /etc/thinstation.global

# Enable Verbose mode of Debug Package
if [ -n "$DEBUG_BOOT" ] ; then
	debug="-d"
fi
if [ -n "$DEBUG_INIT" ]; then
	set -v
fi

if [ -n "$DEBUG_BOOT" ]; then
	echo -n `echo "$PACKAGE -"`" "
	echo "$PACKAGE" >> $LOGFILE
fi

if [ -n "$DEBUG_PACKAGES" ]; then
	2>> $LOGFILE
fi
