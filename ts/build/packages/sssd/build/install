sssd-kcm
sssd-ldap
sssd-nfs-idmap
sssd-proxy
sssd-tools
adcli
oddjob
oddjob-gpupdate
oddjob-mkhomedir"
export DROP_FILES=""
export DROP_DIRS="var/lib/gssproxy/default.sock"
export CREATE_EMPTY_DIRS=""
